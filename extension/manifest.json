{
  "manifest_version": 3,
  "name": "Quick Print",
  "version": "1.0",
  "description": "Alt+click to print links directly",
  "permissions": [
    "activeTab",
    "cookies"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": [
    "<all_urls>"
  ]
} 